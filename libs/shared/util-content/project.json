{
  "name": "util-content",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/util-content/src",
  "projectType": "library",
  "tags": ["type:util", "scope:shared"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/util-content/jest.config.ts"
      }
    },
    "generate-dictionary": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node libs/shared/util-content/scripts/generate-dictionary.js"
      }
    }
  }
}
